Include ~/.ssh/config_private
SendEnv LANG

Host *
	ForwardAgent yes

Host aur.archlinux.org
	User aur

# Check the local network, otherwise go through the vpn
Match host onager exec "not nc -z onager 22 2>/dev/null"
	Hostname onager-vpn

Host testudo
	HostName testudo
	#ProxyJump onager
	User root

Host garden
	User gardener
