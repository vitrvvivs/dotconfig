# file in .config   | destination in $HOME
env                  .env
env                  .bashrc
gitconfig            .gitconfig
profile              .profile
ssh/authorized_keys  .ssh/authorized_keys
ssh/config           .ssh/config
vim                  .vim
vim/vimrc            .vimrc
xinitrc              .xinitrc
zsh/.zshenv          .zshenv
inputrc	             .inputrc

userChrome.css       .mozilla/firefox/*.default/chrome/userChrome.css
userContent.css      .mozilla/firefox/*.default/chrome/userContent.css
